#include <iostream>
using namespace std;

int main()
{
    int a,b;
    cout<<"Enter value of a = ";
    cin>>a;
    cout<<"Enter value of b = ";
    cin>>b;
    
    
    cout<<"\n& Bitwise operator result -> "<<(a&b);
    cout<<"\n| Bitwise operator result -> "<<(a|b);
    cout<<"\n^ Bitwise operator result -> "<<(a^b);

    return 0;
}
