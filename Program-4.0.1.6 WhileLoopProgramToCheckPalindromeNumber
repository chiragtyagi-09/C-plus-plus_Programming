#include <iostream>
using namespace std;

int main()
{
     int n;
     cout<<"\nEnter number => ";
     cin>>n;

        int m=n;
        int rev=0,r;

        while(n>0)
        {
            r=n%10;
            rev=rev*10+r;
            n/=10;
        }

        if (rev==m)
            cout<<"Palindrome";
        else
            cout<<"Not a Palindrome";

return 0;
}
