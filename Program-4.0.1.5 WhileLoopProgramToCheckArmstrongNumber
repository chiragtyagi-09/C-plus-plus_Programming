#include <iostream>
using namespace std;

int main()
{
     int i=1,n,r,sum=0,temp;

     cout<<"Enter number to check whether it is Armstrong or not  : " ;
     cin>>n;
     temp=n;

     while( n>0 )
     {
          r=n%10;    
          sum=sum+(r*r*r);    
          n=n/10;  
     }
     if (temp==sum)
     {
       cout<<"Armstrong number ";
     }
     else
     {
       cout<<"Not an armstrong number";
     }     
return 0;
]
