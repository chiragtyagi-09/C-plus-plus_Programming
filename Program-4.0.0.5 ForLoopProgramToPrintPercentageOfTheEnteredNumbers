#include <iostream>
using namespace std;

int main()
{
    int i,n,x,sum=0;

    cout<<"\nEnter how many values you want to enter : ";
    cin>>n;

    cout<<"\nEnter integer value to find out their percentage : \n";
    for(i=0;i<n;i++)
    {
          cin>>x;
          sum=sum+x;
    }
    
    cout<< "\nPercentage = "<<sum/(float)n<<"\n";

return 0; 
}
