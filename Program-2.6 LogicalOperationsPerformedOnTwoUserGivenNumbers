#include <iostream>
using namespace std;

int main()
{
    int a,b;
    cout<<"Enter value of a = ";
    cin>>a;
    cout<<"Enter value of b = ";
    cin>>b;
    
    
    cout<<"\n&& Logical operator result -> "<<(a&&b);
    cout<<"\n|| Logical operator result -> "<<(a||b);
    cout<<"\n! Logical operator result for a-> "<<(!a);
    cout<<"\n! Logical operator result for b-> "<<(!b);

    return 0;
}
