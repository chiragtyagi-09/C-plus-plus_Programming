#include <iostream>
using namespace std;

int main()
{
     int a[10],n;
     cout<<"Enter how many values you want to enter in array => ";
     cin>>n;
     
     cout<<"Enter values => ";
     for(int i=0;i<n;i++) 
     {
         cin>>a[i];
     }   

     cout<<"Your values stored in array at index places =>\n";

     for(int i=0;i<5;i++) 
     {
           //printf("index value[%d] = %d\n",i,a[i]);
           printf("index value[%d] = %d\n",i,a[i]);

     }
     
return 0;

}
